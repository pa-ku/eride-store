{
  "name": "eride",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "@mercadopago/sdk-react": "^0.0.14",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.477.0",
    "mercadopago": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.30.0",
    "react-router-dom": "^6.30.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^14.1.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.3.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "vite": "^4.5.9"
  },
  "imports": {
    "#src/*": [
      "./src/*"
    ],
    "#components/*": [
      "./src/components/*"
    ],
    "#assets/*": [
      "./src/assets/*"
    ],
    "#services/*": [
      "./src/services/*"
    ],
    "#layout/*": [
      "./src/layout/*"
    ],
    "#pages/*": [
      "./src/pages/*"
    ],
    "#hooks/*": [
      "./src/hooks/*"
    ],
    "#context/*": [
      "./src/context/*"
    ],
    "#utils/*": [
      "./src/utils/*"
    ],
    "#data/*": [
      "./src/data/*"
    ],
    "#lib/*": [
      "./src/libs/*"
    ]
  }
}
