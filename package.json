{
  "name": "eride",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write ."
  },
  "imports": {
    "#src/*": [
      "./src/*"
    ],
    "#components/*": [
      "./src/components/*"
    ],
    "#assets/*": [
      "./src/assets/*"
    ],
    "#services/*": [
      "./src/services/*"
    ],
    "#layout/*": [
      "./src/layout/*"
    ],
    "#pages/*": [
      "./src/pages/*"
    ],
    "#hooks/*": [
      "./src/hooks/*"
    ],
    "#context/*": [
      "./src/context/*"
    ],
    "#utils/*": [
      "./src/utils/*"
    ],
    "#data/*": [
      "./src/data/*"
    ],
    "#lib/*": [
      "./src/libs/*"
    ]
  },
  "dependencies": {
    "@mercadopago/sdk-react": "0.0.14",
    "js-cookie": "^3.0.5",
    "mercadopago": "2.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.14.2",
    "react-router-dom": "6.14.2",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.0.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.38.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "postcss": "8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "3.4.11",
    "vite": "4.3.2"
  }
}
